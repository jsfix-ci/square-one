{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "blocks": {
      "core/column": {
        "spacing": {
          "blockGap": true,
          "margin": true,
          "padding": true
        }
      },
      "core/columns": {
        "border": {
          "radius": true
        },
        "spacing": {
          "blockGap": true,
          "margin": true,
          "padding": true
        }
      },
      "core/image": {
        "border": {
          "radius": true
        }
      },
      "core/media-text": {
        "spacing": {
          "margin": true,
          "padding": true
        }
      }
    },
    "border": {
      "radius": false,
      "color": false,
      "style": false,
      "width": false
    },
    "color": {
      "background": true,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "gradients": [],
      "link": false,
      "text": true
    },
    "custom": {},
    "layout": {
      "contentSize": "800px",
      "wideSize": "1000px"
    },
    "spacing": {
      "blockGap": null,
      "margin": false,
      "padding": false,
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "customFontSize": false,
      "fontStyle": false,
      "fontWeight": false,
      "fluid": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textDecoration": false,
      "textTransform": true,
      "dropCap": true,
      "fontFamilies": [],
      "fontSizes": []
    }
  }
}
